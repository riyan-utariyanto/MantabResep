<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.riyan.MantabResep">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_mantab"
        android:label="Mantab Resep"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.riyan.MantabResep.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.riyan.MantabResep.Nasi"
            android:parentActivityName="com.riyan.MantabResep.MainActivity"></activity>
        <activity android:name="com.riyan.MantabResep.MainActivity" />
    </application>

</manifest>